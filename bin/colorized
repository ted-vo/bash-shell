#!/usr/bin/env bash

# Regular Colors
Black='\e[1;30m'  # Black
Red='\e[1;31m'    # Red
Green='\e[1;32m'  # Green
Yellow='\e[1;33m' # Yellow
Blue='\e[1;34m'   # Blue
Purple='\e[1;35m' # Purple
Cyan='\e[1;36m'   # Cyan
White='\e[1;37m'  # White
NC='\e[0m'        # No Color

# set some colors
CNT="[${Cyan}NOTE${NC}]"
COK="[${Green}OK${NC}]"
CER="[${Red}ERROR${NC}]"
CAT="[${White}ATTENTION${NC}]"
CWR="[${Purple}WARNING${NC}]"
CAC="[${Yellow}ACTION${NC}]"
